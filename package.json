{
  "name": "GeoFiddle",
  "version": "0.10.0",
  "author": "Rui Pinge",
  "homepage": "https://ruipinge.github.io/geofiddle",
  "repository": {
    "type": "git",
    "url": "git@github.com:ruipinge/geofiddle.git"
  },
  "license": "MIT",
  "dependencies": {
    "@material/layout-grid": "^0.40.1",
    "@material/select": "^0.40.1",
    "@material/textfield": "^0.40.1",
    "@material/top-app-bar": "^0.40.1",
    "@material/typography": "^0.40.1",
    "@sentry/browser": "^5.27.2",
    "backbone": "^1.4.0",
    "geodesy": "^1.1.3",
    "google-maps": "^3.3.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "wicket": "^1.3.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "coveralls": "^3.1.0",
    "css-loader": "^6.7.3",
    "cssnano": "^5.1.15",
    "eslint": "^8.36.0",
    "gh-pages": "^5.0.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "node-sass": "^8.0.0",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^8.0.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.2",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.12.0",
    "webpack-merge": "^5.8.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "moduleNameMapper": {
      "^underscore$": "lodash"
    },
    "modulePaths": [
      "<rootDir>/src/",
      "<rootDir>/node_modules"
    ],
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint --ext=mjs,js ./src",
    "lint:fix": "eslint --ext=mjs,js --fix ./src",
    "build": "webpack --config ./src/webpack.prd.mjs",
    "build:dev": "webpack --config ./src/webpack.dev.mjs",
    "start": "webpack-dev-server --progress --config ./src/webpack.dev.mjs",
    "predeploy": "rm -rf ./dist && npm run build && cp ./static/* ./dist/",
    "deploy": "gh-pages -d dist",
    "clean": "rm -rf ./node_modules ./coverage ./npm-debug.log ./dist"
  }
}
