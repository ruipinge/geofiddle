{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
    },
    "env": {
        "browser": true,
        "es6": true,
        "jest": true,
        "commonjs": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-multiple-empty-lines": 2
    },
    "globals": {
        "google": true
    }
}
